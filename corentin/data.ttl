@prefix dcat: <http://www.w3.org/ns/dcat#> .
@prefix dct:  <http://purl.org/dc/terms/> .
@prefix foaf: <http://xmlns.com/foaf/0.1/> .
@prefix gsp:  <http://www.opengis.net/ont/geosparql#> .
@prefix ex:   <http://example.org/> .

ex:c1 a dcat:Catalog ;
    dct:title "Catalog One" ;
    dct:description "Our first catalog" ;
    dct:identifier "c1-abcd-1234" ;
    dct:hasPart ex:r1 , ex:r2 ;
    dcat:landingPage <http://catalog.one/#/public/search> .

ex:r1 a dcat:CatalogRecord ;
    dct:title "Record One" ;
    dct:identifier "r1-abcd-1234" ;
    foaf:primaryTopic ex:d1 .

ex:r2 a dcat:CatalogRecord ;
    dct:title "Record Two" ;
    dct:identifier "r2-abcd-1234" ;
    foaf:primaryTopic ex:d2 .

ex:d1 a dcat:Dataset ;
    dct:title "Dataset One" ;
    dct:description "Our first dataset" ;
    dct:identifier "d1-abcd-1234" ;
    dct:spatial [ a dct:Location ;
                  dcat:bbox "..."^^gsp:wktLiteral ;
                ] .

ex:d2 a dcat:Dataset ;
    dct:title "Dataset Two" ;
    dct:description "Our second dataset" ;
    dct:identifier "d2-abcd-1234" ;
    dct:spatial [ a dct:Location ;
                  dcat:bbox "..."^^gsp:wktLiteral ;
                ] .